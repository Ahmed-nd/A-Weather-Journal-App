  "description": "## Overview This project uses Web API and user data to dynamically update the UI.",
  "homepage": "https://github.com/ahmed-n-abdeltwab/A-Weather-Journal-App#readme",
  "bugs": {
    "url": "https://github.com/ahmed-n-abdeltwab/A-Weather-Journal-App/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmed-n-abdeltwab/A-Weather-Journal-App.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "server.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json,md}\" \"public/**/*.{js,html,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json,md}\" \"public/**/*.{js,html,css}\"",
    "format:public": "prettier --write \"public/**/*.{js,html,css}\""
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "jiti": "^2.4.2",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  }
}
